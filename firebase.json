{
  "functions": {
    "source": "."
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "**/**",
        "function": "next"
      }
    ],
    "ignore": [
      ".firebase/**",
      "firebase.json",
      ".firebaserc",
      "**/node_modules/**",
      "**/.*",
      "**",
      "!public",
      "!i18.json"
    ]
  }
}
